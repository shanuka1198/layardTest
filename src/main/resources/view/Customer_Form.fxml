<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.anjana.controller.CustomerFormController">
   <children>
      <Label layoutX="220.0" layoutY="26.0" prefHeight="38.0" prefWidth="160.0" text="Customer Form">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <TextField fx:id="txtCustId" layoutX="82.0" layoutY="96.0" onAction="#txtBtnCustIdOnAction" prefHeight="25.0" prefWidth="109.0" promptText="ID" />
      <TextField fx:id="txtCustName" layoutX="82.0" layoutY="145.0" prefHeight="25.0" prefWidth="315.0" promptText="Name" />
      <TextField fx:id="txtCustAdd" layoutX="82.0" layoutY="188.0" prefHeight="25.0" prefWidth="309.0" promptText="Address" />
      <TextField fx:id="txtCustTel" layoutX="82.0" layoutY="233.0" prefHeight="25.0" prefWidth="222.0" promptText="Telephone" />
      <Button layoutX="496.0" layoutY="282.0" mnemonicParsing="false" onAction="#btnCustSaveOnAction" text="Save" />
      <Button layoutX="437.0" layoutY="282.0" mnemonicParsing="false" onAction="#btnCustClearOnAction" text="Clear" />
      <Button layoutX="371.0" layoutY="282.0" mnemonicParsing="false" onAction="#btnCustUpdateOnAction" text="Update" />
      <Button layoutX="304.0" layoutY="282.0" mnemonicParsing="false" onAction="#btnCustDeleteOnAction" text="Delete" />
   </children>
</AnchorPane>
